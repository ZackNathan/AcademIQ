{
    "name": "AcademIQ",
    "action": {
        "default_popup": "index.html"
     },
    "manifest_version": 3,
    "version": "0.1",
    "description": "Suggests relevant academic articles based on webpage content",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    }
}
